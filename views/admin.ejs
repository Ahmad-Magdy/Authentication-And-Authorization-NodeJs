    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>update Page</title>
    <link rel="stylesheet" href="/css/boostrap.min.css">

</head>
<body>
<div class="container">
    <br><br><br>
    <table class="table table-striped table-bordered ">
        <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Email</th>
            <th>username</th>
            <th>Bio</th>
            <th>Role</th>
            <th></th>
        </tr>
        </thead>
        <tobdy>
            <%for(var i = 0 ; i < users.length; i++){%>
            <tr>
                <td></td>
                <td><%=users[i].name%></td>
                <td><%=users[i].email%></td>
                <td><%=users[i].username%></td>
                <td><%=users[i].bio%></td>
                <td><%=users[i].Role%></td>
                <td><a class="btn btn-info" href="/admin/role/<%=users[i].username%>">Change Role</a></td>
            </tr>
            <%}%>
        </tobdy>
    </table>
</div>
</body>
</html>
